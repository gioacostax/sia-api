{
  "env": {
    "browser": true,
    "node": true
  },

  "extends": "gio",

  "globals": {
    "Promise": true
  },

  "rules": {
    "strict": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "max-nested-callbacks": 0,
    "max-depth": 0,
    "no-underscore-dangle": 0,
    "id-length": 0
  }
}
